{
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "rust-analyzer.serverPath": "/usr/local/bin/rust-analyzer",
  "diagnostic-languageserver.filetypes": {
    "text": ["proselint"],
    "markdown": ["proselint"]
  },
  "diagnostic-languageserver.linters": {
    "proselint": {
      "command": "proselintjs",
      "isStdout": true,
      "isStderr": false,
      "debounce": 100,
      "args": ["-"],
      "offsetLine": 0,
      "offsetColumn": 0,
      "sourceName": "proselint",
      "formatLines": 1,
      "formatPattern": [
        "^[^:]+:(\\d+):(\\d+):(\\s)(.*)$",
        {
          "line": 1,
          "column": 2,
          "security": 3,
          "message": 4
        }
      ],
      "securities": {
        " ": "info"
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": false,
    "trailingComma": "all"
  }
}
